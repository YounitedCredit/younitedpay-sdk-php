{
  "name": "202ecommerce/younitedpay-sdk",
  "description": "younited pay API SDK",
  "homepage": "https://api.younited-pay.com/",
  "license": "AFL-3.0",
  "authors": [
      {
          "name": "202 ecommerce",
          "email": "tech@202-ecommerce.com"
      }
  ],
  "config": {
      "platform": {
          "php": "5.6"
      },
      "vendor-dir": "vendor",
      "preferred-install": "dist",
      "classmap-authoritative": true,
      "optimize-autoloader": true,
      "prepend-autoloader": false
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "require": {
    "php": ">=5.6.0",
    "psr/log": "^3.0",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
      "php": ">=5.6",
      "friendsofphp/php-cs-fixer": "^3.2",
      "squizlabs/php_codesniffer": "^3.4",
      "phpunit/phpunit": "^9.0",
      "phpstan/phpstan": "^0.12.81"
  },
  "psr-4": {
    "Tot\\YounitedPaySDK\\": "src"
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  }
}
